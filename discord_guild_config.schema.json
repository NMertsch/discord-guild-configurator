    },
    "system_channel": {
      "$ref": "#/$defs/SystemChannel"
    },
    "categories": {
      "items": {
        "$ref": "#/$defs/Category"
      },
      "title": "Categories",
      "type": "array"
    },
    "community_features": {
      "anyOf": [
        {
          "$ref": "#/$defs/CommunityFeatures"
        },
        {
          "type": "null"
        }
      ]
    },
    "verification_level": {
      "enum": [
        "none",
        "low",
        "medium",
        "high",
        "highest"
      ],
      "title": "Verification Level",
      "type": "string"
    },
    "default_notifications": {
      "enum": [
        "all_messages",
        "only_mentions"
      ],
      "title": "Default Notifications",
      "type": "string"
    },
    "explicit_content_filter": {
      "enum": [
        "disabled",
        "no_role",
        "all_members"
      ],
      "title": "Explicit Content Filter",
      "type": "string"
