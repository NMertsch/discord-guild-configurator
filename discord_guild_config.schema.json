  "$defs": {
    "Category": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "channels": {
          "items": {
            "discriminator": {
              "mapping": {
                "forum": "#/$defs/ForumChannel",
                "text": "#/$defs/TextChannel",
                "voice": "#/$defs/VoiceChannel"
              },
              "propertyName": "type"
            },
            "oneOf": [
              {
                "$ref": "#/$defs/TextChannel"
              },
              {
                "$ref": "#/$defs/ForumChannel"
              },
              {
                "$ref": "#/$defs/VoiceChannel"
              }
            ]
          },
          "title": "Channels",
          "type": "array"
        },
        "permission_overwrites": {
          "items": {
            "$ref": "#/$defs/PermissionOverwrite"
          },
          "title": "Permission Overwrites",
          "type": "array"
        }
      },
      "required": [
        "name",
        "channels"
      ],
      "title": "Category",
      "type": "object"
    },
    "ForumChannel": {
      "additionalProperties": false,
      "properties": {
        "type": {
          "const": "forum",
          "default": "forum",
