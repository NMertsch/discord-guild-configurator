          "title": "Permission Overwrites",
          "type": "array"
        },
        "tags": {
          "items": {
            "type": "string"
          },
          "title": "Tags",
          "type": "array"
        },
        "require_tag": {
          "default": false,
          "title": "Require Tag",
          "type": "boolean"
        }
      },
      "required": [
        "name",
        "topic"
      ],
      "title": "ForumChannel",
      "type": "object"
    },
