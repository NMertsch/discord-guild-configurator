{
  "$defs": {
    "Category": {
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "channels": {
          "items": {
            "discriminator": {
              "mapping": {
                "forum": "#/$defs/ForumChannel",
                "text": "#/$defs/TextChannel",
                "voice": "#/$defs/VoiceChannel"
              },
              "propertyName": "type"
            },
            "oneOf": [
              {
                "$ref": "#/$defs/TextChannel"
              },
              {
                "$ref": "#/$defs/ForumChannel"
              },
              {
                "$ref": "#/$defs/VoiceChannel"
              }
            ]
          },
          "title": "Channels",
          "type": "array"
        },
        "permission_overwrites": {
          "items": {
            "$ref": "#/$defs/PermissionOverwrite"
          },
          "title": "Permission Overwrites",
          "type": "array"
        }
      },
      "required": [
        "name",
        "channels"
      ],
      "title": "Category",
      "type": "object"
    },
    "CommunityFeatures": {
      "additionalProperties": false,
      "properties": {
        "guild_description": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "title": "Guild Description"
        },
        "rules_channel": {
          "title": "Rules Channel",
          "type": "string"
        },
        "public_updates_channel": {
          "title": "Public Updates Channel",
          "type": "string"
        },
        "safety_alerts_channel": {
          "title": "Safety Alerts Channel",
          "type": "string"
        }
      },
      "required": [
        "guild_description",
        "rules_channel",
        "public_updates_channel",
        "safety_alerts_channel"
      ],
      "title": "CommunityFeatures",
      "type": "object"
    },
    "ForumChannel": {
      "additionalProperties": false,
      "properties": {
        "type": {
          "const": "forum",
          "default": "forum",
          "title": "Type",
          "type": "string"
